{
  "name": "michaelhall/twig-console-dump",
  "description": "Twig extension for dumping a variable to browser console.",
  "license": "MIT",
  "keywords": [
    "twig",
    "dump",
    "console",
    "extension",
    "debug"
  ],
  "authors": [
    {
      "name": "Michael Hall",
      "homepage": "https://michaelhall.se/"
    }
  ],
  "require": {
    "php": ">=7.1",
    "twig/twig": "^2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.5||^6.0||^7.0"
  },
  "autoload": {
    "psr-4": {
      "MichaelHall\\TwigConsoleDump\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MichaelHall\\TwigConsoleDump\\Tests\\": "tests"
    }
  }
}
